{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"platforms": ["android"],
	"permissions": [
		"core:default",
		"core:app:default",
		"core:app:allow-version",
		"dialog:default",
		"fs:default",
		"opener:default",
		"geolocation:allow-check-permissions",
		"geolocation:allow-request-permissions",
		"geolocation:allow-get-current-position",
		"android-mock-location:default",
		"android-mock-location:allow-checkPermissions",
		"android-mock-location:allow-requestPermissions"
	]
}